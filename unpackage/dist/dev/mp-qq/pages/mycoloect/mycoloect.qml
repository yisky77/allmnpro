<view class="container"><block wx:if="{{cartList.length>0}}"><view class="cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="cart-item"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class="image-wrapper" bindtap="__e"><image src="{{item.picUrl}}"></image><view data-event-opts="{{[['tap',[['check',['item',index]]]]]}}" class="{{['yticon icon-xuanzhong2 checkbox',(item.checked)?'checked':'']}}" bindtap="__e"></view></view><view class="item-right"><text data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class=" title" bindtap="__e">{{item.name}}</text><text data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class="attr" bindtap="__e">{{item.brief}}</text><text data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class="price" bindtap="__e">{{"¥"+item.retailPrice}}</text><label data-event-opts="{{[['tap',[['deletes',['$0'],[[['cartList','',index,'id']]]]]]]}}" class="deletebtn _span" bindtap="__e">取消收藏</label></view></view></block></block></view></block><block wx:else><view class="ectouch-notcont" style="margin-top:130rpx;"><view class="img"><image mode="aspectFit" src="/static/category/no_content.png"></image></view><text class="cont">亲,没有相关内容</text></view></block></view>