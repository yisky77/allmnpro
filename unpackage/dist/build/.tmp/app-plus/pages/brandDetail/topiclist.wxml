<view class="container"><block wx:if="{{topiclist.length>0}}"><view class="topic-list"><block wx:for="{{topiclist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="item" url="{{'../brandDetail/topicDetail?id='+item.id}}"><image class="img" src="{{item.picUrl}}"></image><view class="info"><text class="title">{{item.title}}</text><text class="desc">{{item.subtitle}}</text><text class="price">{{item.price+"元起"}}</text></view></navigator></block></view></block><block wx:else><view class="ectouch-notcont" style="padding-top:100rpx;"><view class="img"><image mode="aspectFit" src="/static/category/no_content.png"></image></view><text class="cont">亲,暂无相关专题内容</text></view></block></view>