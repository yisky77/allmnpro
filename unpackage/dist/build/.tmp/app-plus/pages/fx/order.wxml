<view class="content"><view class="navbar"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{['nav-item',(tabCurrentIndex==index)?'current':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><view class="tab-content"><scroll-view class="list-scroll-content" scroll-y><block wx:if="{{orderList.length>0}}"><view><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navTo',['$0'],[[['orderList','',index,'id']]]]]]]}}" class="order-item" bindtap="__e"><view class="i-top b-b"><text class="time">{{"订单号:"+item.orderSn}}</text><text class="state" style="{{'color:'+(item.stateTipColor)+';'}}">{{item.orderStatusText}}</text><block wx:if="{{item.state===9}}"><text data-event-opts="{{[['tap',[['deleteOrder',[index]]]]]}}" class="del-btn yticon icon-iconfontshanchu1" bindtap="__e"></text></block><block wx:if="{{item.state===4}}"><text class="del-btn yticon icon-iconfontshanchu1">去评价</text></block></view><block wx:for="{{item.goodsList}}" wx:for-item="goodsItem" wx:for-index="goodsIndex" wx:key="goodsIndex"><view class="goods-box-single"><image class="goods-img" src="{{goodsItem.picUrl}}" mode="aspectFill"></image><view class="right"><text class="title clamp">{{goodsItem.goodsName}}</text><text class="attr-box">{{goodsItem.goodsName+"  x "+goodsItem.number}}</text></view></view></block><view class="price-box">实付款<text class="price">{{item.actualPrice}}</text></view></view></block></view></block><block wx:else><view class="ectouch-notcont" style="margin-top:130rpx;"><view class="img"><image mode="aspectFit" src="/static/category/no_content.png"></image></view><text class="cont">亲,暂无当前状态的订单哦</text></view></block></scroll-view></view></view>