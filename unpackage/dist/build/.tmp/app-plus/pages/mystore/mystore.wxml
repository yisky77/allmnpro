<view class="container"><view class="cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="cart-item"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class="image-wrapper" bindtap="__e"><image src="{{item.logo_thumb}}"></image><view data-event-opts="{{[['tap',[['check',['item',index]]]]]}}" class="{{['yticon icon-xuanzhong2 checkbox',(item.checked)?'checked':'']}}" bindtap="__e"></view></view><view class="item-right"><text data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class="clamp title" bindtap="__e">{{item.store_name}}</text><text class="attr">{{"已有"+item.collectnum+"人关注"}}</text><label data-event-opts="{{[['tap',[['deletes',['$0'],[[['cartList','',index,'store_id']]]]]]]}}" class="deletebtn _span" bindtap="__e">取消关注</label></view></view></block></block></view></view>